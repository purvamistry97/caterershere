<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>Orders</title>
	<link href="/static/stylesheets/caterer/bootstrap.min.css" rel="stylesheet">
	<link href="/static/stylesheets/caterer/font-awesome.min.css" rel="stylesheet">
	<link href="/static/stylesheets/caterer/datepicker3.css" rel="stylesheet">
	<link href="/static/stylesheets/caterer/styles.css" rel="stylesheet">
	<link href="/static/stylesheets/caterer/datatables.css" rel="stylesheet">
	<link href="/static/stylesheets/caterer/datatables.min.css" rel="stylesheet">
	<!--Custom Font-->
	<link href="https://fonts.googleapis.com/css?family=Montserrat:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
	<!--[if lt IE 9]>
	<script src="js/html5shiv.js"></script>
	<script src="js/respond.min.js"></script>
	<![endif]-->
</head>
<body>
	<nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
		<div class="container-fluid">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-collapse"><span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span></button>
				<a class="navbar-brand" href="#"><span>Caterer</span></a>
			</div>
		</div><!-- /.container-fluid -->
	</nav>
	<div id="sidebar-collapse" class="col-sm-3 col-lg-2 sidebar">
		<div class="profile-sidebar">
			<div class="profile-userpic">
				<img src="http://placehold.it/50/30a5ff/fff" class="img-responsive" alt="">
			</div>
			<div class="profile-usertitle">
				<div class="profile-usertitle-name"><%= Caterer[0].Name%></div>
				<div class="profile-usertitle-status"><span class="indicator label-success"></span>Online</div>
			</div>
			<div class="clear"></div>
		</div>
		<div class="divider"></div>
		<form role="search">
			<div class="form-group">
				<input type="text" class="form-control" placeholder="Search">
			</div>
		</form>
		<ul class="nav menu">
			<li><a href="/catererdashboard"><em class="fa fa-dashboard">&nbsp</em> Dashboard</a></li>
			<li class="active"><a href="/catererorder"><em class="fa fa-calendar">&nbsp</em> Orders</a></li>
			<li><a href="/catererprofile"><em class="fa fa-bar-chart">&nbsp</em> Profile</a></li>
			<li><a href="/catererhome"><em class="fa fa-power-off">&nbsp</em> Logout</a></li>
		</ul>
	</div><!--/.sidebar-->
		
	<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
		<div class="row">
			<ol class="breadcrumb">
				<li><a href="#">
					<em class="fa fa-home"></em>
				</a></li>
				<li class="active">Orders</li>
			</ol>
		</div><!--/.row-->
		
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">Orders</h1>
			</div>
		</div><!--/.row-->
		<div>
			<table id="example" class="display" style="width:100%">
				<thead>
					<tr>
						<th>First Name</th>
						<th>Last Name</th>
						<th>Address</th>
						<th>No Of People</th>
						<th>Package</th>
						<th>Inquiry</th>
						<th>Event</th>
						<th>Event Date</th>
					</tr>
				</thead>
				<tbody>
					<% orderList.forEach((item, index)=>{%> 
						<tr>
							<td><%= item.FirstName%></td>
							<td><%= item.LastName%></td>
							<td><%= item.Address%></td>
							<td><%= item.Peoples%></td>
							<td><%= item.PackageName%></td>
							<td><%= item.Inquiry%></td>
							<td><%= item.Event%></td>
							<td><%= item.EventDate%></td>
						</tr>
					<%})%>
				</tbody>
				<tfoot>
					<tr>
						<th>First Name</th>
						<th>Last Name</th>
						<th>Address</th>
						<th>No Of People</th>
						<th>Package</th>
						<th>Inquiry</th>
						<th>Event</th>
						<th>Event Date</th>
					</tr>
				</tfoot>
			</table>
		
		</div>
	</div>	<!--/.main-->

	<script src="/static/javascripts/caterers/jquery-1.11.1.min.js"></script>	
	<script src="/static/javascripts/caterers/datatables.js"></script>
	<script src="/static/javascripts/caterers/datatables.min.js"></script>
	<script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>
	
	<script src="/static/javascripts/caterers/bootstrap.min.js"></script>
	<script src="/static/javascripts/caterers/chart.min.js"></script>
	<script src="/static/javascripts/caterers/chart-data.js"></script>
	<script src="/static/javascripts/caterers/easypiechart.js"></script>
	<script src="/static/javascripts/caterers/easypiechart-data.js"></script>
	<script src="/static/javascripts/caterers/bootstrap-datepicker.js"></script>
	<script src="/static/javascripts/caterers/custom.js"></script>
	<script>
		$(document).ready(function() {
    		$('#example').DataTable( {
    		   /*  "columnDefs": [
    		        {
    		            // The `data` parameter refers to the data for the cell (defined by the
    		            // `data` option, which defaults to the column being worked with, in
    		            // this case `data: 0`.
    		            "render": function ( data, type, row ) {
    		                return data +' ('+ row[3]+')'
    		            },
    		            "targets": 0
    		        },
    		        { "visible": false,  "targets": [ 3 ] }
    		    ] */
    		} )
		} )
	</script>
</body>
</html>
