    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>customerHome</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link href="/static/stylesheets/customer/others/bootstrap.min.css" rel="stylesheet">
	    <link href="/static/stylesheets/customer/others/datepicker3.css" rel="stylesheet">
	    <link href="/static/stylesheets/customer/others/styles.css" rel="stylesheet">
    </head>
    <body>
        <div class="row">
            <div class="col-xs-10 col-xs-offset-1 col-sm-8 col-sm-offset-2 col-md-4 col-md-offset-4">
                <div class="login-panel panel panel-default">
                    <div class="panel-heading">Register</div>
                    <div class="panel-body">
                        <form role="form" method="POST" action="/register">
                            <fieldset>
                                <div class="form-group">
                                    <input class="form-control" placeholder="First Name" name="firstName" type="text" autofocus="">
                                </div>
                                <div class="form-group">
                                    <input class="form-control" placeholder="Last Name" name="lastName" type="text" autofocus="">
                                </div>
                                <div class="form-group">
                                    <input class="form-control" placeholder="E-mail" name="email" type="email" autofocus="">
                                </div>
                                <div class="form-group">
                                    <input class="form-control" placeholder="Password" name="password" type="password" value="">
                                </div>
                                <div class="checkbox">
                                    <label>
                                        <input name="remember" type="checkbox" value="Remember Me">Remember Me
                                    </label>
                                </div>
                               <input type="submit" value="Register" class="btn btn-primary" />
                                <a href="/customerhome" class="btn btn-primary" style="margin-left: 2%">Login</a>
                                <p style="color:red"><%=message%></p>
                            </fieldset>
                        </form>
                    </div>
                </div>
                <div style="margin-left: 39%;"><a href="/" style="color: white">Enter as guest</a></div>
                
            </div><!-- /.col-->
        </div><!-- /.row -->	
        <!--[if lt IE 7]>
            <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="#">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
        
        <script src="" async defer></script>
        <script src="/static/javascripts/customer/jquery-1.11.1.min.js"></script>
        <script src="/static/javascripts/customer/bootstrap.min.js"></script>
    </body>
</html>